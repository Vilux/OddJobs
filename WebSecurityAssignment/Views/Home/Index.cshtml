@{
    ViewData["Title"] = "Home Page";
}
<div>
    <section class="job_feed">
        <div id="feed_list">
            <h1>Odd Job Feed</h1>
            <ul>
                <li>Mow my lawn</li>
                <li>Pick up groceries</li>
                <li>Need car washed</li>
                <li>Drop/Pickup Dry Cleaning</li>
            </ul>
        </div>
        <div id="feed_details">

            <section id="details">
                <h3>Mow my lawn</h3>
                <p>
                    Need my lawn mowed since I hurt my back and cant push the mower anymore.
                    Should take no longer than afew hours
                </p>
                <p>Start Date: Anytime </p>
